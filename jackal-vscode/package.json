{
  "name": "jackal-lang",
  "displayName": "Jackal Language Support",
  "description": "Syntax highlighting, snippets, icon theme, and color theme for the Jackal programming language.",
  "publisher": "alegrarsio",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jackal",
        "aliases": ["Jackal", "jackal"],
        "extensions": [".jackal"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jackal",
        "scopeName": "source.jackal",
        "path": "./syntaxes/jackal.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jackal",
        "path": "./snippets/jackal.code-snippets"
      }
    ],
    "iconThemes": [
      {
        "id": "jackal-icon-theme",
        "label": "Jackal Icon Theme",
        "path": "./icons/jackal-icon-theme.json"
      }
    ],
    "themes": [
      {
        "label": "Jackal Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/jackal-color-theme.json"
      }
    ]
  }
}
