import std.File.csv.CsvStream

class Csv : CsvStream {

    func load() -> Csv {
        this.result = __csv_read(this.path)
        return this
    }

    func select(item : Array<String>) -> Csv{
        this.result = __csv_select(this.result,item)
        return this
    }
    
    func exports() -> Array = 
        this.result or "Error"

    
}